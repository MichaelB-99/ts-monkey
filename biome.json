{
	"$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter":{
		"enabled": true
	},
	"linter": {
		"enabled": true,
		
		"rules": {
			"correctness":{
				"noUnusedImports":{
					"fix":"safe",
					"level":"warn"
				}
			},
			"recommended": true
		}
	}
}
